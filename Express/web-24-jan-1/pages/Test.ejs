<html>
    <head>
        <title>Home Page..</title>
    </head>
    <body>
        <h1>Test Page called...</h1>
        <b>Array is :</b> <%= arr %><br><br>
        <hr>
        <%   
        for(let i=0;i<arr.length;i++){ 
            if(arr[i]%5==0){
                arr[i] = arr[i]*3;
            }else{
                arr[i] = arr[i];
            }
        %>
        <%
            }
        %>
        <b>New Array is : </b><%= arr %>
        <hr>

        <% 
        let sum = 0;   
        for(let i=0;i<arr.length;i++){ 
            if(i%2!=0){
                sum += arr[i];
            }
        %>
        <%
            }
        %>
        <b>Sum is : </b><%= sum %>
        <hr>
        <%   
        for(let i=0;i<arr.length/2;i++){ 
                let temp = arr[i];
                arr[i] = arr[arr.length-(i+1)]; 
                arr[arr.length-(i+1)] = temp;
        %>
        <%
            }
        %>
        <b>Reversed Array : </b><%= arr %>

    </body>
</html>